{% extends "guest/base.html.twig" %}

{% block title %}Liste des produits
{% endblock %}

{% block main %}
	<main>
		<h2>Liste des produits</h2>
		{% for product in products %}
				<article>
					<h3>
						<a href="{{ path('detail-product', {'id':product.id}) }}">{{product.title}}</a>
					</h3>
				</article>
		{% endfor %}
	</main>
{% endblock main %}
